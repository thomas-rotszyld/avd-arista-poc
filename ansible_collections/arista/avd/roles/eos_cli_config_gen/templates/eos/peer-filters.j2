{# eos - peer filters #}
{% for peer_filter in peer_filters | arista.avd.natural_sort('name') %}
!
peer-filter {{ peer_filter.name }}
{%     for sequence in peer_filter.sequence_numbers | arista.avd.natural_sort('sequence') %}
